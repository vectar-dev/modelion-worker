{
    "compilerOptions": {
        "module": "commonjs",
        "esModuleInterop": true,
        "target": "es6",
        "noImplicitAny": false,
        "moduleResolution": "node",
        "sourceMap": true,
        "pretty": true,
        "outDir": "dist",
    },
    "resolveJsonModule": true,
    "include": [
        "src/**/*.ts",
    ],
    "exclude": [
        "node_modules"
    ]
}